{
  "name": "edgeruler",
  "version": "0.1.0",
  "description": "A small library for dual-constraining a Delaunator triangulation",
  "type": "module",
  "main": "./dist/EdgeRuler.cjs",
  "module": "./dist/EdgeRuler.mjs",
  "types": "./dist/EdgeRuler.d.ts",
  "exports": {
    ".": {
      "import": "./dist/EdgeRuler.mjs",
      "require": "./dist/EdgeRuler.cjs",
      "types": "./dist/EdgeRuler.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@types/delaunator": "^3.0.0",
    "@types/node": "^20.11.16",
    "@vitest/coverage-v8": "^1.2.2",
    "delaunator": "^5.0.0",
    "robust-segment-intersect": "^1.0.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vite-plugin-dts": "^3.7.2",
    "vitest": "^1.2.2"
  },
  "dependencies": {
    "robust-predicates": "^3.0.1"
  }
}
