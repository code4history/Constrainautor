{
    "name": "@kninnug/constrainautor",
    "version": "4.0.0",
    "description": "A small library for constraining a Delaunator triangulation",
    "type": "module",
    "main": "./dist/Constrainautor.cjs",
    "module": "./dist/Constrainautor.mjs",
    "types": "./dist/Constrainautor.d.ts",
    "exports": {
        ".": {
            "import": "./dist/Constrainautor.mjs",
            "require": "./dist/Constrainautor.cjs",
            "types": "./dist/Constrainautor.d.ts"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "preview": "vite preview",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:coverage": "vitest run --coverage",
        "typecheck": "tsc --noEmit"
    },
    "keywords": [
        "constrained",
        "delaunay",
        "triangulation",
        "Delaunator"
    ],
    "author": "Marco Gunnink",
    "license": "ISC",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/kninnug/constrainautor.git"
    },
    "devDependencies": {
        "@types/delaunator": "^5.0.3",
        "@types/node": "^20.11.16",
        "@types/robust-segment-intersect": "^1.0.2",
        "@vitest/coverage-v8": "^1.2.2",
        "robust-predicates": "^3.0.1",
        "robust-segment-intersect": "^1.0.1",
        "typescript": "^5.3.3",
        "vite": "^5.0.12",
        "vite-plugin-dts": "^3.7.2",
        "vitest": "^1.2.2"
    },
    "peerDependencies": {
        "delaunator": "^5.0.0"
    }
}
